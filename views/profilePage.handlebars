{{!-- {{userDb.dataValues.firstName}}
{{userDb.dataValues.lastName}} --}}

{{#if userDb.posts}}
	{{this.title}}
	{{this.body}}
{{/if}}

{{#each userDb}}
		<div class="row">
			<div class="col-md-12">
				<h2>{{this.firstName}}</h2>
			</div>
		</div>
		<div class="row">
			{{#if this.posts}}
				{{#each this.posts}}
						{{> profileReview}}
					</div>
				{{/each}}
			{{/if}}
		</div>
	{{/each}}

	<div class="col-md-12">
    <form id="posts">
      <div class="form-group">
        <label for="title">Title</label>
        <input placeholder="Post Title" type="text" class="form-control" id="title">
        <label for="body">Body</label>
        <textarea placeholder="Body" class="form-control" rows="10" id="postBody"></textarea>
        <div class="form-group">
          <label for="category">Select Place:</label>
          <select class="form-control" id="place"></select>
        </div>
        <button type="submit" class="btn btn-success submit">Submit</button>
      </div>
    </form>
  </div>



<div class="row">
    <div class="col-xl-12 text-center">
    	<img src="{{profile.image}}" class="img-thumbnail rounded-circle">
    </div>
</div>
<div class="row">
    <div class="col-xl-12 text-center mt-4">
    	<h1>{{profile.name}}</h1>
    </div>
</div>
<br>
<hr>
<br>

<div class="row">
	<div class="col-lg-8 reviewHolder">
        {{> profileReview}}

	</div>
</div>
